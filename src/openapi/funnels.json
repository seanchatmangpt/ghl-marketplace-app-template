                    "target": {
                        "type": "string",
                        "example": "https://www.example.com/new-path",
                        "description": "Target URL to which the original path will be redirected"
                    },
                    "action": {
                        "type": "string",
                        "example": "url",
                        "description": "Action performed by the redirect"
                    }
